        errorHouse.textContent= "House name is required.";
    }
    if (!landmark) {
        isValid = false;
        errorLand.textContent= "Landmark is required.";
    }
    if (!city) {
        isValid = false;
        errorCity.textContent= "City is required.";
    }
    if (!country) {
        isValid = false;
        errorCountry.textContent= "Please select Country.";
    }
    if (!state) {
        isValid = false;
        errorState.textContent= "state name is required.";
    }
    if (!pin || !/^[1-9]\d{5}$/.test(pin)) {
        isValid = false;
        errorZip.textContent= "Valid 6 digit PIN is required.";
    }

    if (!isValid) return;

    const addressData = {
            // name: name,
            // alternatePhone: mobile,
            houseName: house,
            landmark: landmark,
            city:city,
            state:state,
            country:country,
            pin:pin
        };

        //console.log(addressData)

    // If form is valid, submit it using Fetch API
    try {
        const response = await fetch('/editAddress/<%= address._id %>', {
            method: 'PUT',
            headers: {
                    'Content-Type': 'application/json'
                },
            body: JSON.stringify(addressData)
        });

        if (response.ok) {
            const result = await response.json();
            console.log('Address Updated successfully');
            window.location.href = '/address';
        } else {
            console.log('Failed to save address');
        }
    } catch (error) {
        console.log('An error occurred: ' + error.message);
    }
});

</script>
