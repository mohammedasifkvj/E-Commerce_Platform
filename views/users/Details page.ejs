<script>
  // Add to cart
  async function addProductToCart(productId) {
    const response = await fetch('/addProductToCart', {
      method: "POST",
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({
        productId
      })
    });

    // window.location.href = response.url;
    if (response.redirected) {
      Swal.fire({
        icon: "warning",
        title: "You are not Signd in",
        text: "Please Sign-in to add products to your cart",
        showConfirmButton: false,
        timer: 2000
      });
    }

    const result = await response.json();

    if (response.status === 401) {
      Swal.fire({
        icon: "warning",
        title: "You are not Signd in",
        text: "Please Sign-in to add products to your cart",
        showConfirmButton: false,
        timer: 2000
      });
    } else if (response.status === 200) {
      if (result.message === 'Product already exists in cart') {
        Swal.fire({
          icon: "info",
          title: "Product already exists in cart",
          //text: "Product quantity updated",
          showConfirmButton: false,
          timer: 1300
        });
      } else {
        Swal.fire({
          icon: "success",
          title: "Product added to cart",
          showConfirmButton: false,
          timer: 1300
        });
      }
    } else if (response.status === 400 && result.message === 'Product is not available') {
      Swal.fire({
        icon: "error",
        title: "Product not available",
        text: "This product is out of stock",
        showConfirmButton: true
      });
    } else {
      Swal.fire({
        icon: "error",
        title: "Error",
        text: result.error || "An error occurred",
        showConfirmButton: true
      });
    }
  }

  // Add to Wishtist 
  async function addProductToWishlist(productId) {
    const response = await fetch('/addProductToWishlist', {
      method: "POST",
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({
        productId
      })
    });

    if (response.redirected) {
      Swal.fire({
        icon: "warning",
        title: "You are not Signd in",
        text: "Please Sign-in to add products to your Wishlist",
        showConfirmButton: false,
        timer: 2000
      });
    }
    const result = await response.json();

    if (response.status === 401) {
      Swal.fire({
        icon: "warning",
        title: "You are not Signd in",
        text: "Please Sign-in to add products to your Wishlist",
        showConfirmButton: false,
        timer: 2000
      });
    } else if (response.status === 200) {
      if (result.message === 'Product already exists in Wishlist') {
        Swal.fire({
          icon: "info",
          title: "Product already exists in Wishlist",
          showConfirmButton: false,
          timer: 1300
        });
      } else {
        Swal.fire({
          icon: "success",
          title: "Product added to Wishlist",
          showConfirmButton: false,
          timer: 1300
        });
      }
    } else {
      Swal.fire({
        icon: "error",
        title: "Error",
        text: result.error || "An error occurred",
        showConfirmButton: true
      });
    }
  }
</script>
<script>
  // review form submission
  document.getElementById('reviewForm').addEventListener('submit', async function(event) {
    event.preventDefault();

    // Clear previous errors
    const errorElements = document.querySelectorAll('.error-message');
    errorElements.forEach(el => el.textContent = '');

    const rating = document.getElementById('rating').value;
    const name = document.getElementById('name').value;
    const productName = document.getElementById('productName').value;
    const comments = document.getElementById('comments').value;

    let isValid = true;

    // Rating validation
    if (!rating) {
      document.getElementById('ratingError').textContent = 'Rating must be selected.';
      isValid = false;
    }
    // Product name validation
    if (!name) {
      document.getElementById('nameError').textContent = 'Enter Your name here';
      isValid = false;
    }
    // comments validation
    if (!comments) {
      document.getElementById('commentsError').textContent = 'Write the comments about the product';
      isValid = false;
    }
    if (!isValid) return;

    const reviewData = {
      rating: parseInt(rating),
      name: name,
      comments: comments,
      productName: productName
    }
    // If validation passes, submit the form
    try {
      const response = await fetch('/product/review/<%=product._id %>', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(reviewData)
      });

      if (response.ok) {
        // Handle successful response
        const data = await response.json();
        Swal.fire({
          icon: "success",
          title: data.message,
          showConfirmButton: false,
          timer: 2000
        });
        window.location.href = `/product/<%=product._id %>`
      } else {
        // Handle errors from the server
        const errorData = await response.json();
        throw new Error('Network response was not ok');
      }
    } catch (error) {
      console.error('There was a problem with the fetch operation:', error);
    }
  });
</script>
