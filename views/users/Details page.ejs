<script>
  const containers = [
    document.getElementById("img-contain"),
    document.getElementById("img-contain1"),
    document.getElementById("img-contain2"),
    document.getElementById("img-contain3")
  ];

  const imgs = [
    document.getElementById("photos"),
    document.getElementById("photos1"),
    document.getElementById("photos2"),
    document.getElementById("photos3")
  ];

  containers.forEach((container, index) => {
    const img = imgs[index];
    container.addEventListener('mousemove', (e) => {
      const rect = container.getBoundingClientRect();
      const x = e.clientX - rect.left;
      const y = e.clientY - rect.top;
      img.style.transformOrigin = `${x}px ${y}px`;
      img.style.transform = "scale(2)";
    });

    container.addEventListener('mouseleave', () => {
      img.style.transformOrigin = "center center";
      img.style.transform = "scale(1)";
    });
  });
</script>
