                <span class="breadcrumb-label" itemprop="name">Order Details</span>
            <meta itemprop="position" content="4" />
        </li>
    </ul>
    <div class="container container-smaller">
        <div class="page-content">
            <div class="themevale-heading">
                <h1 class="page-heading title">Order Details</h1>
            </div>

            <nav class="navBar navBar--sub navBar--account">
                <ul class="navBar-section">
                    <li class="navBar-item">
                        <a class="navBar-action" href="/account">Account</a>
                     </li>
                     <li class="navBar-item">
                        <a class="navBar-action" href="/address">Addresses</a>
                     </li>
                     <li class="navBar-item">
                         <a class="navBar-action" href="/orders">Orders</a>
                     </li>
                     <li class="navBar-item ">
                        <a class="navBar-action" href="/wishlist">Wish Lists</a>
                     </li>
                     <li class="navBar-item">
                         <a class="navBar-action" href="/profileSettings">Account Settings</a>
                     </li>
                 </ul>
            </nav>

            <div class="account">
                <div class="account-body">
                    <div class="account-content account-content--fixedSmall">
                        <p class="account-heading">Order ID:<%= order._id %></p>

<ul>
    <% orderItems.forEach(item => { %>
    <li class="account-listItem">
        <div class="account-product account-product--alignMiddle">
          <!--  <div class="account-product-checkItem">
                    <input class="form-checkbox" type="checkbox" id="account-product-id-15" value="15">
                     <label for="account-product-id-15" class="form-label">
                        <span class="is-srOnly">Checkbox 15 label</span>
                    </label>
            </div> -->
            <figure class="account-product-figure">
                <img src="<%= item.productId?.productImage[0] %>" alt="<%= item.productId.productName %>" title="<%= item.productId.productname %>" data-sizes="auto"/>
            </figure>
            <div class="account-product-body">
                <span class="account-product-price"><%= item.quantity * item.productId.discountPrice %></span>
                <h5 class="account-product-title"><%= item.quantity %> &#215; <%= item.productId.productName %></h5>
                <dl class="definitionList">
                        <!-- <dt class="definitionList-key">Color:</dt>
                        <dd class="definitionList-value">White</dd>
                        <dt class="definitionList-key">Size:</dt>
                        <dd class="definitionList-value">S</dd> -->
                </dl>
            </div>
        </div>
    </li>
    <% }) %> 
</ul>

                        <dl class="account-orderTotal">
                                    <!-- <dt class="account-orderTotal-key">Subtotal:</dt>
                                    <dd class="account-orderTotal-value">1,847.99</dd> 
                                    <dt class="account-orderTotal-key">Shipping:</dt>
                                    <dd class="account-orderTotal-value">â‚¹10.00</dd>-->
                                    <dt class="account-orderTotal-key">Grand total:</dt>
                                    <dd class="account-orderTotal-value"><%= orderItems.reduce((total, item) => total + (item.quantity * item.productId.discountPrice), 0) %></dd>
                        </dl>
                    </div>

                    <aside class="account-sidebar">
                        <section class="account-sidebar-block">
                            <h3 class="account-heading">Order Details</h3>
                            <dl class="definitionList">
                                <dt class="definitionList-key">Ordered On:</dt>
                                    <dd class="definitionList-value">
                                        <%= (order.createdAt).toDateString() %>
                                    </dd>
                                <dt class="definitionList-key">Order total:</dt>
                                <dd class="definitionList-value"><%= orderItems.reduce((total, item) => total + (item.quantity * item.productId.discountPrice), 0) %></dd>
                                    <dt class="definitionList-key">Payment method:</dt>
                                    <dd class="definitionList-value">
                                        Cash on Delivery
                                    </dd>
                                    <dt class="definitionList-key">Order Status:</dt>
                                    <dd class="definitionList-value">
                                        <%= order.status %>
                                    </dd>
                                   <!-- <dt class="order-payments-description">Here&#x27;s how to pay for your order:</dt>
                                     <dd class="order-payments-value order-payments-value--long">Type your cash on delivery instructions in here.</dd> -->
                            </dl>
                           
                            <!-- <button data-print-invoice="/account.php?action=print_invoice&amp;order_id=107" class="button button--small">Print Invoice</button>-->
                        </section>

                        <section class="account-sidebar-block">
                            <h3 class="account-heading">Bill To</h3>
                            <ul class="account-order-address">
                                <li><%= user?.name%></li>
                                <li><%= user?.email%></li>
                                <li><%= address?.mobile%></li>
                            </ul>
                        </section>

                        <section class="account-sidebar-block">
                            <h3 class="account-heading">Ship To</h3>
                            <ul class="account-order-address">
                                <!-- <li><%= address?.name%></li> -->
                                <li><%= address?.houseName%></li>
                                <li><%= address?.landmark%></li>
                                <li><%= address?.state%></li>
                                <li><%= address?.country%></li>
                                <li><%= address?.pin %></li>
                            </ul>
                        </section>
                     <!--   <section class="account-sidebar-block">
                            <h3 class="account-heading">Order Comments</h3>
                            <p>second order</p>
                        </section>

                        <section class="account-sidebar-block">
                            <h3 class="account-heading">Actions</h3>
                            <div class="order-details-info">
                                 <form class="account-reorder-form" action="/account.php?action=reorder&amp;order_id=107" method="post" data-account-reorder-form>
                                    <input type="hidden" name="orderid" value="107">
                                            <input type="hidden" name="reorderitem[15]">
                                            <input type="hidden" name="reorderitem[14]">
                                    <input type="submit" class="button button--small" value="Reorder">
                                </form> -->
                            </div>
                        </section>
                    </aside>

                </div>
            </div>
        </div>
    </div>
