                </svg></div> Print -->
            </a>
            <a href="/invoiceDownload/<%= order?._id %>" class="button button--tertiary optimizedCheckout-buttonSecondary">Invoice Download</a>
          </header>
          
          <section class="cart-section optimizedCheckout-orderSummary-cartSection">
            <% orderItems.forEach(item => { %>
            <!-- <h3 class="cart-section-heading optimizedCheckout-contentPrimary" data-test="cart-count-total">5 Items</h3> -->
            <ul aria-live="polite" class="productList">
              <li class="productList-item is-visible">
                <div class="product" data-test="cart-item">
                  <figure class="product-column product-figure"><img alt="<%= item.productId.productName %>" data-test="cart-item-image" src="<%= item.productId?.productImage[0] %>"></figure>
                  <div class="product-column product-body">
                    <h4 class="product-title optimizedCheckout-contentPrimary" data-test="cart-item-product-title"><%= item.quantity %> x <%= item.productId.productName %></h4>
                    <ul class="product-options optimizedCheckout-contentSecondary" data-test="cart-item-product-options">
                      <li class="product-option" data-test="cart-item-product-option"><%= item.productId.discountPrice %></li>
                      <!-- <li class="product-option" data-test="cart-item-product-option">Size S</li> -->
                    </ul>
                  </div>
                <div class="product-column product-actions">
                  <div class="product-price optimizedCheckout-contentPrimary" data-test="cart-item-product-price"><%= item.quantity * item.productId.discountPrice %></div>
                </div>
                </div>
              </li>
            </ul>
            <% }) %>
          </section>
          <section class="cart-section optimizedCheckout-orderSummary-cartSection">
            <div data-test="cart-subtotal">
              <div aria-live="polite" class="cart-priceItem optimizedCheckout-contentPrimary cart-priceItem--subtotal"><span class="cart-priceItem-label"><span data-test="cart-price-label">Subtotal </span></span><span class="cart-priceItem-value"><span data-test="cart-price-value"><%= orderItems.reduce((total, item) => total + (item.quantity * item.productId.discountPrice), 0) %></span></span></div>
            </div>
            <div data-test="cart-shipping">
              <div aria-live="polite" class="cart-priceItem optimizedCheckout-contentPrimary"><span class="cart-priceItem-label"><span data-test="cart-price-label">Discount </span></span><span class="cart-priceItem-value"><span data-test="cart-price-value"><%= order?.discount %></span></span></div>
            </div>
            <!-- <div data-test="cart-taxes">
